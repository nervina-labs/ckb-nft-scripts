import blockchain;
import common;

struct CompactNFTId {
  issuer_id: IssuerId,
  class_id: Uint32,
  token_id: Uint32,
}

vector CompactNFTIdVec <CompactNFTId>;

table CompactNFTInfo {
	characteristic: Characteristic,
	configure: byte,
	state: byte,
	receiver_lock: Bytes,
}

vector CompactNFTInfoVec <CompactNFTInfo>;

table CompactNFTMintEntries {
  nft_ids: CompactNFTIdVec,
  nft_infos: CompactNFTInfoVec,
  proof: Bytes,
}